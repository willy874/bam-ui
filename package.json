{
  "name": "bam-ui",
  "version": "0.0.24",
  "license": "MIT",
  "repository": "https://github.com/willy874/bam-ui",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:vue": "vite build -c compile/vue-config.ts",
    "start": "docker-compose up",
    "preview": "vite preview"
  },
  "bin": {
    "bam-ui": "script/bundle.js"
  },
  "files": [
    "packages",
    "dist",
    "pkg-vue",
    "types"
  ],
  "type": "module",
  "main": "./dist/bam-ui.es.js",
  "module": "./dist/bam-ui.es.js",
  "types": "./dist/packages/vue/index.d.ts",
  "exports": {
    "./vue": {
      "module": "./pkg-vue/bam-ui.es.js",
      "import": "./pkg-vue/bam-ui.es.js",
      "require": "./pkg-vue/bam-ui.umd.js",
      "script": "./pkg-vue/bam-ui.umd.js",
      "types": "./pkg-vue/packages/vue/index.d.ts",
      "style": "./pkg-vue/style.css",
      "scss": "./packages/core/style/style.scss"
    }
  },
  "typesVersions": {
    "*": {
      "vue": [
        "pkg-vue/packages/vue/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "bam-utility-plugins": "^0.2.2"
  },
  "peerDependencies": {
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vitejs/plugin-legacy": "^1.6.3",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.1.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-html": "^1.2.0",
    "postcss-less": "^5.0.0",
    "prettier": "^2.4.1",
    "sass": "^1.44.0",
    "stylelint": "^14.0.1",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-order": "^5.0.0",
    "tailwindcss": "^3.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3",
    "typescript-plugin-css-modules": "^3.4.0",
    "vite": "^2.6.4",
    "vite-plugin-dts": "^0.9.9",
    "vite-plugin-purge-icons": "^0.7.0",
    "vite-plugin-vue-setup-extend": "^0.1.0",
    "vue": "^3.2.16",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.29.3"
  }
}
